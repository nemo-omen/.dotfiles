[calendars]

[[private]]
path = /home/trainingmontage/.local/share/khal/calendars/private
type = calendar

[locale]
timeformat = %I:%M %p
dateformat = %m/%d/%Y
longdateformat = %m/%d/%Y
datetimeformat = %m/%d/%Y %I:%M %p
longdatetimeformat = %m/%d/%Y %I:%M %p

[default]
default_calendar = private

[highlight_days]
color = dark magenta
method = background
